}

.contact-layout {
    display: grid;
    grid-template-columns: 320px 1fr;
    gap: 40px;
}

.contact-sidebar {
    display: flex;
    flex-direction: column;
}

.contact-form-wrapper {
    background: var(var(--accent-blue));
